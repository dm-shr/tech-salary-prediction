[MASTER]
disable=
    C0114, # missing-module-docstring
    C0115, # missing-class-docstring
    C0116, # missing-function-docstring
    W0613, # unused-argument
    E1101, # no-member
    E0611, # no-name-in-module
    E0401, # import-error
    W0201, # attribute-defined-outside-init
    W0719, # broad-exception-too-many-ancestors
    W1514, # unspecified-encoding
    C0413, # wrong-import-position
    C0301, # line-too-long
    W0718, # broad-exception-caught
    C0103, # invalid-name
    R0903, # too-few-public-methods
    R0902, # too-many-instance-attributes
    W0621, # redefined-outer-name
    R0913, # too-many-arguments
    R0914, # too-many-locals
    R0915, # too-many-statements
    R0912, # too-many-branches
    W0612, # unused-variable
    R1705, # no-else-return
    W0123, # eval-used
    R1733, # unnecessary-dict-index-lookup
    W1203, # logging-fstring-interpolation
    R0917, # too-many-positional-arguments
    R0402, # consider-using-from-import
    R0801, # duplicate-code

extension-pkg-whitelist=numpy,torch,sklearn
good-names=i,j,k,ex,Run,_,X,y,df,x1,x2,f,e,tp,tn,fp,fn,x,z,q,p,v

[TYPECHECK]
ignored-modules=numpy,torch,sklearn
ignored-classes=numpy,torch,sklearn
generated-members=numpy.*,torch.*

[FORMAT]
max-line-length=150

[DESIGN]
max-args=10
max-attributes=20
max-locals=30
max-statements=75
max-branches=25
